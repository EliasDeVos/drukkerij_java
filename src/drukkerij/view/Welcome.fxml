<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?scenebuilder-stylesheet mainviewStyle.css?>
<?import java.net.URL?>

<AnchorPane prefHeight="100" prefWidth="50" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="drukkerij.controller.WelcomeController">
    <stylesheets>
        <URL value="@mainviewStyle.css" />
    </stylesheets>
   <children>
      <GridPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="393.0" minWidth="10.0" prefWidth="252.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="335.0" minWidth="10.0" prefWidth="335.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="358.0" minWidth="10.0" prefWidth="358.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="414.0" minWidth="10.0" prefWidth="248.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label GridPane.columnIndex="1" GridPane.rowIndex="1" fx:id="hostname" text="Database host name"/>
            <Button mnemonicParsing="false" text="Change" GridPane.columnIndex="2" GridPane.rowIndex="1" onAction="#changeHostname"/>
            <Button mnemonicParsing="false" text="Jo" GridPane.columnIndex="1" GridPane.rowIndex="2" onMouseClicked="#createOverviewJo" />
            <Button mnemonicParsing="false" text="Martine" GridPane.columnIndex="2" GridPane.rowIndex="2" onMouseClicked="#createOverviewMartine" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
