<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="drukkerij.controller.RootLayoutController" stylesheets="@mainviewStyle.css">
    <top>
        <MenuBar>
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Close" onAction="#handleClose"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Lijst">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#showDrukOrderMartine" text="Martine"/>
                    </items>
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#showDrukOrderJo" text="Jo"/>
                    </items>
                </Menu>
            </menus>
        </MenuBar>
    </top>
    <stylesheets>
        <URL value="@mainviewStyle.css"/>
    </stylesheets>
</BorderPane>
